{
	"name": "homebridge-unifi-protect-webhook",
	"version": "0.2.7",
	"displayName": "UniFi Protect Webhook",
	"description": "Homebridge plugin: expose local webhook endpoints and email triggers as HomeKit Motion sensors for UniFi Protect/Reolink integration.",
	"main": "src/index.js",
	"keywords": [
		"homebridge-plugin",
		"homebridge",
		"unifi",
		"protect",
		"webhook",
		"email",
		"imap",
		"gmail"
	],
	"engines": {
		"node": ">=20.0.0",
		"homebridge": ">=1.8.0 || ^2.0.0"
	},
	"homebridge": {
		"pluginAlias": "ProtectWebhookPlatform",
		"pluginType": "platform",
		"schema": "./config.schema.json"
	},
	"ui": "homebridge-ui/server.js",
	"homepage": "https://github.com/tommasocere/homebridge-unifi-protect-webhook#readme",
	"bugs": {
		"url": "https://github.com/tommasocere/homebridge-unifi-protect-webhook/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TommasoCere/homebridge-unifi-protect-webhook.git"
	},
	"scripts": {
		"audit": "npm audit --omit=dev"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"src/*.js",
		"config.schema.json",
		"README.md",
		"README.it.md",
		"CHANGELOG.md",
		"LICENSE",
		"homebridge-ui/"
	],
	"dependencies": {
		"body-parser": "2.2.0",
		"express": "5.1.0",
		"imapflow": "1.1.1",
		"uuid": "13.0.0",
		"@homebridge/plugin-ui-utils": "2.1.0"
	},
	"devDependencies": {},
	"author": "Tommaso Ceredi",
	"license": "MIT"
}
